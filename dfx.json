{
  "canisters": {
    "bundle_package": {
      "main": "src/content-bundle/BundlePackage.mo",
      "type": "motoko",
      "dependencies": []
    },
    "package_registry": {
      "main": "src/content-bundle-registry/PackageRegistry.mo",
      "type": "motoko",
      "dependencies": []
    },
    "package_service": {
      "main": "src/content-bundle-service/PackageService.mo",
      "type": "motoko",
      "dependencies": []
    },
    "content-bundle-tools": {
      "frontend": {
        "entrypoint": "src/content-bundle-tools/build/index.html"
      },
      "source": [
        "src/content-bundle-tools/build"
      ],
      "type": "assets",
      "build": "npm run build:content-bundle-tools"
    }
  },
  "defaults": {
    "build": {
      "packtool": "mops sources"
    }
  },  
  "version": 1
}
